{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dbml",
      "camelcase": "Dbml",
      "title": "DBML",
      "scope": "source.dbml",
      "file-types": [
        "dbml"
      ],
      "injection-regex": "^dbml$",
      "class-name": "TreeSitterDbml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "DBML Parser",
    "authors": [
      {
        "name": "Gerry Sihaj",
        "email": "gsihaj5@gmail.com",
        "url": "https://github.com/gsihaj5/tree-sitter-dbml"
      }
    ],
    "links": {
      "repository": "https://github.com/gsihaj5/tree-sitter-dbml",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}